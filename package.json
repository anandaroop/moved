{
  "name": "change",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "mapbox-gl": "^0.44.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-keyframes": "https://github.com/anandaroop/react-keyframes.git#passthru-duration",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.14",
    "@storybook/addon-links": "^3.3.14",
    "@storybook/react": "^3.3.14",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "prettier": "^1.10.2",
    "react-scripts": "1.1.1",
    "vcards-js": "^2.8.0"
  },
  "homepage": "http://anandaroop.github.io/change",
  "lint-staged": {
